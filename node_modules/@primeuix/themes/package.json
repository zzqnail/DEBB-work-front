{
  "name": "@primeuix/themes",
  "version": "1.1.1",
  "author": "PrimeTek Informatics",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primefaces/primeuix.git",
    "directory": "packages/themes"
  },
  "bugs": {
    "url": "https://github.com/primefaces/primeuix/issues"
  },
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./types/*": {
      "types": "./types/*/index.d.ts"
    },
    "./tokens": {
      "types": "./tokens/index.d.ts",
      "import": "./tokens/index.mjs"
    },
    "./*": {
      "types": "./*/index.d.ts",
      "import": "./*/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@primeuix/styled": "^0.6.1"
  }
}